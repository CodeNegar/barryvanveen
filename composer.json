{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "cebe/markdown": "~1.1.0",
    "cviebrock/eloquent-sluggable": "2.*",
    "doctrine/dbal": "2.*",
    "flyingfoxx/commandcenter": "1.2.*",
    "laracasts/flash": " 1.2",
    "laracasts/validation": "1.*",
    "laracasts/utilities": "1.*",
    "laravel/framework": "4.2.*",
    "laravelrus/localized-carbon": "1.*",
    "lavary/laravel-menu": "1.*",
    "robclancy/presenter": "1.3.*",
    "swisnl/head": "dev-master"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "1.*",
    "barryvdh/laravel-ide-helper": "1.*",
    "codeception/codeception": "2.0.*",
    "fzaninotto/faker": "1.4.*",
    "laracasts/testdummy": "1.*",
    "phpunit/phpunit": "4.*",
    "way/generators": "2.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:swisnl/head.git"
    }
  ],
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/database/migrations",
      "app/database/seeds",
      "app/models"
    ],
    "psr-4": {
      "Barryvanveen\\": "app/models"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
