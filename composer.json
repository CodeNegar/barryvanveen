{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "bepsvpt/secure-headers": "^3.0",
    "bugsnag/bugsnag-laravel": "2.4.*",
    "cebe/markdown": "1.1.*",
    "cviebrock/eloquent-sluggable": "4.2.*",
    "doctrine/dbal": "2.*",
    "eusonlito/laravel-meta": "3.0.*",
    "guzzlehttp/guzzle": "6.*",
    "laracasts/flash": "2.0.*",
    "laracasts/utilities": "2.*",
    "laravel/framework": "5.4.*",
    "laravelcollective/html": "5.4.*",
    "laravelcollective/remote": "5.4.*",
    "league/flysystem-dropbox": "1.0.*",
    "league/glide": "1.2.*",
    "mccool/laravel-auto-presenter": "5.0.*",
    "php": ">=5.5.9",
    "rap2hpoutre/laravel-log-viewer": "0.8.*",
    "roumen/feed": "2.10.*",
    "spatie/laravel-googletagmanager": "2.2.*",
    "symfony/css-selector": "^3.0",
    "symfony/dom-crawler": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "2.3.*",
    "barryvdh/laravel-ide-helper": "2.2.*",
    "friendsofphp/php-cs-fixer": "1.12.*",
    "fzaninotto/faker": "1.6.*",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "laravel/browser-kit-testing": "^1.0",
    "laravel/dusk": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Barryvanveen\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
