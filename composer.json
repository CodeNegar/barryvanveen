{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "cebe/markdown": "1.1.*",
    "cviebrock/eloquent-sluggable": "3.0.0-beta",
    "doctrine/dbal": "2.*",
    "eusonlito/laravel-meta": "2.0.*",
    "guzzlehttp/guzzle": "6.*",
    "laracasts/flash": "1.3.*",
    "laracasts/utilities": "2.*",
    "laravel/framework": "5.1.*",
    "laravelrus/localized-carbon": "dev-master",
    "league/flysystem-dropbox": "1.*",
    "league/glide": "0.3.*",
    "mccool/laravel-auto-presenter": "3.1.*",
    "php": ">=5.5.9",
    "rap2hpoutre/laravel-log-viewer": "0.3.*",
    "thujohn/rss": "1.0.*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "2.0.*",
    "barryvdh/laravel-ide-helper": "2.0.*",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Barryvanveen\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
